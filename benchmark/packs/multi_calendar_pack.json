{
  "pack_id": "multi_calendar_v1",
  "version": "1.0",
  "description": "Plugin conversion checks across non-BS calendars.",
  "calendar_family": "multi_calendar",
  "cases": [
    {
      "id": "islamic-convert-2026-02-15",
      "endpoint": "/v2/api/engine/convert",
      "params": {"date": "2026-02-15", "calendar": "islamic"},
      "assertions": {
        "result.year": 1447,
        "result.month": 8,
        "result.day": 27
      },
      "tolerance": 0,
      "source": "Plugin validation baseline"
    },
    {
      "id": "hebrew-convert-2026-02-15",
